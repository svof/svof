            <script>-- limbcounters prompt tag is defined here
-- feel free to tinker with it, but move it out into a script of its own,
-- so your changes don't get erased on an update!
function svo.kl_prompttag2()
  if svo.defc.dragonform or not svo.lasthit or not svo.kl_list[svo.lasthit] then return &quot;&quot; end
  local t = svo.kl_list[svo.lasthit]
  return string.format(&quot;%s: h %d|t %d|ra %d|la %d|rl %d|ll %d&quot;, svo.lasthit, t.head, t.torso, t.rightarm, t.leftarm, t.rightleg, t.leftleg)
end
svo.adddefinition(&quot;@kl_prompttag2&quot;, &quot;svo.kl_prompttag2()&quot;)



function svo.kl_prompttag()
  if svo.defc.dragonform or not svo.lasthit or not svo.kl_list[svo.lasthit] then return &quot;&quot; end
  local t = svo.kl_list[svo.lasthit]
  local prep = svo.conf.limbprep
  return string.format(&quot;%s%d&lt;a_white&gt;/%s%d&lt;a_white&gt;|%s%d&lt;a_white&gt;/%s%d&lt;a_white&gt;|%s%d&lt;a_white&gt;/%s%d&lt;a_white&gt;|%d&quot;,
                   ((t.kl_break_at - t.leftarm) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.leftarm,
                   ((t.kl_break_at - t.rightarm) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.rightarm,
                   ((t.kl_break_at - t.leftleg) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.leftleg,
                   ((t.kl_break_at - t.rightleg) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.rightleg,
                   ((t.kl_break_at - t.head) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.head,
                   ((t.kl_break_at - t.torso) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.torso, t.kl_break_at)
end
svo.adddefinition(&quot;@kl_prompttag&quot;, &quot;svo.kl_prompttag()&quot;)


function svo.kl_prompttag3()
  if svo.defc.dragonform or not svo.lasthit or not svo.kl_list[svo.lasthit] then return &quot;&quot; end
  local t = svo.kl_list[svo.lasthit]
  local prep = svo.conf.limbprep
  return string.format(&quot;&lt;a_darkgrey&gt;(%s%d&lt;a_darkgrey&gt;:%s%d&lt;a_darkgrey&gt;:%s%d&lt;a_darkgrey&gt;:%s%d&lt;a_darkgrey&gt;:%s%d&lt;a_darkgrey&gt;:%s%d&lt;a_darkgrey&gt;:%d)&quot;,
                   ((t.kl_break_at - t.leftarm) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.leftarm,
                   ((t.kl_break_at - t.rightarm) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.rightarm,
                   ((t.kl_break_at - t.leftleg) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.leftleg,
                   ((t.kl_break_at - t.rightleg) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.rightleg,
                   ((t.kl_break_at - t.head) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.head,
                   ((t.kl_break_at - t.torso) &lt;= prep) and &quot;&lt;a_red&gt;&quot; or &quot;&lt;a_darkyellow&gt;&quot;, t.torso, t.kl_break_at)
end
svo.adddefinition(&quot;@kl_prompttag3&quot;, &quot;svo.kl_prompttag3()&quot;)</script>
