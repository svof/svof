elseif maxhp &gt;= 6350 then guesstimate = 14
elseif maxhp &gt;= 5700 then guesstimate = 13
elseif maxhp &gt;= 5100 then guesstimate = 12
elseif maxhp &gt;= 4500 then guesstimate = 11
else guesstimate = 10
end

tempTimer(0, function()

  svo.echof(
